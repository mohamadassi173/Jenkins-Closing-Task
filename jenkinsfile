pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                    sh 'mvn package'
            }
        }
         stage('Run') {
            steps {
                 sh 'sudo nohup java -jar target/xmltohtml-0.0.1-SNAPSHOT.jar &'
            }
}


            
            
}
            post {

                success {
                        slackSend channel: '#fursa-5', message: 'built successful !', teamDomain: 'fursa-5', tokenCredentialId: 'slack'}

            }
}
