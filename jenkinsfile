pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                    sh 'mvn package'
            }
        }
         stage('Run') {
            steps {
                 sh 'JENKINS_NODE_COOKIE=do_not_kill nohup java -jar target/xmltohtml-0.0.1-SNAPSHOT.jar'
            }
}
}
}
